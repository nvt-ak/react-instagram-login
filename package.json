{
  "name": "@amraneze/react-instagram-login",
  "version": "3.0.3",
  "description": "An Instagram Login Component for React",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack/webpack.config.dev.js",
    "demo": "webpack serve --config webpack/webpack.config.demo.js",
    "build": "set NODE_ENV=production&& webpack --config webpack/webpack.config.prod.js",
    "build:dev": "webpack --config webpack/webpack.config.dev.js",
    "watch": "webpack -w",
    "check-types": "tsc",
    "test": "mocha --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "security": "yarn audit; [[ $? -ge $SEVERITY ]] && exit 1 || exit 0",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prettier:fix": "yarn prettier --write .",
    "prepare": "husky install",
    "clean": "rm -rf node_modules dist; rm package-lock.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amraneze/react-instagram-login.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "instagram-login",
    "instagram-oAuth2",
    "instagram-oAuth"
  ],
  "author": "Amrane Ait Zeouay <a.zeouayamrane@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Amraneze/react-instagram-login/issues"
  },
  "homepage": "https://github.com/Amraneze/react-instagram-login",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.3",
    "chai": "4.1.2",
    "chai-jquery": "2.0.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-risxss": "^2.1.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^7.0.4",
    "jquery": "^3.5.1",
    "jsdom": "^18.1.0",
    "lint-staged": "^12.0.2",
    "mocha": "^8.2.1",
    "prettier": "^2.4.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "directories": {
    "test": "test"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1"
  }
}
